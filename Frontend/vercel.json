{
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ],
  
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          { "key": "Access-Control-Allow-Origin", "value": "https://recipe-community-frontend.vercel.app" },
          { "key": "Access-Control-Allow-Credentials", "value": "true" },
          { "key": "Cache-Control", "value": "no-store" }
        ]
      }
    ]
  }


  