{
    "version": 2,
    "builds": [
      {
        "src": "Backend/**/*.js",
        "use": "@vercel/node"
      },
      {
        "src": "Frontend/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "build"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "Frontend/$1"
      },
      {
        "src": "/backend/(.*)",
        "dest": "Backend/$1"
      }
    ]
  }
  